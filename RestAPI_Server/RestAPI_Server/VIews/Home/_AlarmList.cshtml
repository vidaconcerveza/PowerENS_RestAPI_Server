
@{
    ViewData["Title"] = "_AlarmList";
}
@model ChartViewModel
<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Message</th>
            <th>View</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model.Charts)
        {
            <tr>
                <td>@t.LogTime</td>
                <td>@t.AlarmMessage</td>
                <td><a asp-controller="Home" asp-action="Detail" asp-route-id="@t.Id">DETAIL</a></td>
            </tr>
        }
    </tbody>
</table>
